<!-- List all admins in the database-->
<html>
  <head>
  </head>
  <body>
    <h1>Admins</h1>

    <table>
      <thead>
        <tr>
          <td>Name</td>
          <td>Email></td>
        </tr>
      </thead>
      <tbody>
        <% @admins.each do |admin|%>
        <tr>
          <td><%= admin.name %></td>
          <td><%= admin.email %></td>
          <td><%= link_to "Edit", edit_admin_path(admin) %></td>
        </tr>
      <%end%>
    </tbody>
  </table>
  <br/><%= link_to "New Admin", new_admin_path %>
</body>
</html>
