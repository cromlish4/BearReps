<h1> <%= link_to "CSE #{@courseID}", (courses_url + "/#{@section.courseID}")%></h1>
<h1><%= "Class : #{@section.classNumber}" %></h1>
<!--TODO, display Course number-->

<div class="section">
  <div class=""container>
    <p> <%= "Status : #{@section.enrollmentStatus}" %> </p>
    <p> <%= "Instruction Mode : #{@section.instructionMode}" %> </p>
    <p> <%= "Component : #{@section.component}" %> </p>
    <p> <%= "Class Number : #{@section.classNumber}" %> </p>
    <p> <%= "Section : #{@section.section}" %> </p>
    <p> <%= "Meeting Days : #{@section.meetingDays}" %> </p>
    <p> <%= "Meeting Time : #{@section.meetingTimes}" %> </p>
    <p> <%= "wait list : #{@section.waitlistTotal}" %> </p>
    <p> <%= "Class Period : #{@section.startDate} - #{@section.endDate}" %></p>
    <p> <%= "Grader : #{@section.grader}" %></p>

  </div>
</div>
<%if (current_user.verified == "true") && ((current_user.user_type == "admin")||(current_user.user_type == "dev"))%>
<%= button_to "Delete Section", @section, :method => :delete, data: {confirm: "===WARNING===\n!!! ATTEMPTING TO DELETE SECTION"} %>
<%= button_to "Edit Section", "#{admin_graders_edit_path}?", :method => :get, params: {:id => params[:id], :section => true} %>
<%end %>